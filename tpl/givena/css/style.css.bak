	margin: 0 30px 0 0;
}

.cont-text table tr th{
	padding: 10px 0;
	border-bottom: 1px solid #80b61b;
	text-align: left;
	font-weight: bold;
	color: #0a5100;
}

.cont-text table tr td{
	padding: 10px 0;
	border-bottom: 1px solid #80b61b;
}

.cont-text a:visited{
	color: #b28073;
}

.gallery{
