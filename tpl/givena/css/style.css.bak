html, body {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
}

body{
	position:relative;
	min-height: 100%;
	height: auto !important;
	height: 100%;
	padding: 0px;
	margin: 0px;
	font-family: Arial;
	color: #010c00;
	font-size: 12px;
	/*line-height: 110%;*/
}

.head{
	background: #c7e28b;
	height: 168px;
	border-top: 15px solid #fff;
}

.head .logo{
	margin: 20px 65px 0 65px;
	float: left;
	border: 0;
}

.head .head-img{
	float: left;
}

.head .head-img img{
	margin: 19px 22px 0 0;
	float: left;
	border: 5px solid #fff;
}

.content {
  padding-bottom: 120px;
}

.menu-left {
	width: 200px;
	float: left;
	padding: 0 0 0 30px;
}

.content .menu-left .phone{
	margin: 0;
	padding: 0;
	text-align: center;
	color: #0a5100;
	font-size: 16px;
	border-bottom: 1px solid #80b61b;
	height: 42px;
}

.content .menu-left .phone p{
	margin: 0;
	padding: 11px 0 0 0;
}

.content .menu-left .services{
	height: 54px;
}

.content .menu-left .services .home{
	float: left;
	width: 13px;
	height: 12px;
	background:url('../images/home.png') left top no-repeat;
	margin: 20px 0 0 40px;
}

.content .menu-left .services .letter{
	float: left;
	width: 12px;
	height: 12px;
	background:url('../images/letter.png') left top no-repeat;
	margin: 20px 0 0 40px;
}

.content .menu-left .services .mapsite{
	float: left;
	width: 10px;
	height: 12px;
	background:url('../images/mapsite.png') left top no-repeat;
	margin: 20px 0 0 40px;
}

.content .menu-left .menu-l{
	background:url('../images/menu-left-bg.gif') left bottom no-repeat #e5f5bb;
	padding: 8px 0 30px 0;
}

.menu-left-1lvl{
	
}

.menu-left-1lvl .lvl1{
	color: #0a5100;
	text-transform: uppercase;
	font-size: 13px;
	text-decoration: none;
	font-weight: bold;
	display: block;
	padding: 3px 20px;
	background: #c7e28b;
	margin: 5px 0;
}

.menu-left-1lvl .lvl1.act{
	color: #010c00;
}

.menu-left-1lvl .lvl1:hover{
	color: #0a5100;
	text-decoration: none;
}

.lvl2{
	display: block;
	text-decoration: none;
	background: url("../images/menu-bullet-2.png") 20px 7px no-repeat;
	padding: 3px 20px 3px 40px;
	color: #0a5100;
	margin: 3px 0;
	font-size: 13px;
}

.lvl2.act{
	background: url("../images/menu-bullet-2-act.png") 20px 7px no-repeat #c7e28b;
	color: #010c00;
}

.lvl3{
	display: block;
	text-decoration: none;
	background: url("../images/menu-bullet-3.png") 38px 9px no-repeat;
	padding: 3px 20px 3px 49px;
	color: #0a5100;
	margin: 5px 0;
	font-size: 13px;
	border-left: 1px solid #e5f5bb;
	border-right: 1px solid #e5f5bb;
}

.lvl3.act{
	background: url("../images/menu-bullet-3-act.png") 38px 9px no-repeat #fff;
	color: #010c00;
	border-left: 1px solid #e5f5bb;
	border-right: 1px solid #e5f5bb;
}



.cont-block {
	width: 720px;
	float: left;
	padding: 0 0 0 30px;
}

.content .cont-block .menu-top {
	border-bottom: 1px solid #80b61b;
	padding: 0 0 0 10px;
	height: 42px;
	position: relative;
	margin: 0 30px 0 0;
}

.content .cont-block .menu-top ul {
	padding: 0;
	margin: 0;
}

.content .cont-block .menu-top ul li {
	float: left;
	position: relative;
	list-style: none;
	padding: 0;
	margin: 0;
}

.content .cont-block .menu-top ul li ul {
	position: absolute;
	left: 0;
	top: 25px;
	z-index: 2;
	width: 200px;
	margin: 0;
	padding: 18px 0 0 0;
	display: none;
}

.content .cont-block .menu-top ul li:hover ul {
	display: block;
}

.content .cont-block .menu-top ul li ul li{
	list-style-type: none;
	background: #e5f5bb;
	display: block;
	padding: 0 0 5px 0;
	
}

.content .cont-block .menu-top ul li ul li:first-child{
	padding: 10px 0 5px 0;
	
}

.content .cont-block .menu-top ul li ul li:last-child{
	padding: 0 0 10px 0;
	
}

.content .cont-block .menu-top ul li ul li a{
	text-transform: uppercase;
	display: block;
	width: 164px;
	float: none;
	padding: 3px 18px;
	font-weight: bold;
	font-size: 13px;
}

.content .cont-block .menu-top ul li ul li a:hover{
	background: #c7e28b;
	color: #0a5100;
}

.content .cont-block .menu-top a {
	color: #0a5100;
	text-decoration: none;
	padding: 12px 17px;
	font-size: 14px;
	float: left;
}

.content .cont-block .menu-top a:hover {
	color: #80b61b;
}

.content .search-basket-top{
	height: 39px;
	padding: 15px 30px 0 0;
}

.content .search-basket-top .search{
	float: left;
}

.content .search-basket-top .search .text{
	background: #e5f5bb;
    border: 0 none;
    float: left;
    font-size: 12px;
    height: 24px;
    margin: 0;
    padding: 0 10px;
    width: 140px;
	color: #0a5100;
}

.content .search-basket-top .search .butt{
	background: url("../images/loop.png") no-repeat scroll left top transparent;
    border: 0 none;
    cursor: pointer;
    float: left;
    height: 24px;
    width: 25px;
}

.content .search-basket-top .basket{
	float: right;
	color: #0a5100;
	background: url("../images/bag.png") no-repeat scroll left top transparent;
	padding: 0px 0 2px 23px;
}

.content .search-basket-top .basket span{
	float: left;
	padding: 4px 10px 0 0;
}

.content .search-basket-top .basket  .goto-basket{
	float: left;
	margin: 1px 0 0 0;
	background: url("../images/goto-basket-1.png") no-repeat scroll left top transparent;
	width: 114px;
	height: 23px;
	border: 0;
}

.content .search-basket-top .basket  .goto-basket:hover{
	background: url("../images/goto-basket.png") no-repeat scroll left top transparent;
}

.footer {
  height: 96px;
  width: 100%;
  position: absolute;
  left:0;
  bottom: 0;
  border-bottom: 8px solid #c7e28b;
}

.inn{
	position:relative;
	padding: 0px;
	margin: 0px auto;
	width: 980px;
}

.footer div.creater{
	background:url('../images/logo-creater.png') left top no-repeat;
	position: absolute;
	right: 0px;
	top: 20px;
	text-align: left;
	margin: 0px;
	font-size: 10px;
	width: 141px;
	height: 45px;
}

.footer div.creater p{
	line-height: 10px;
	margin: 3px 0 0 0;
	padding: 0 0 0 63px;
}

.footer div.creater p a{
	color: #0a5100;
	text-decoration: none;
}

.footer div.creater p a:hover{
	text-decoration: underline;
}

.footer .adress{
	color: #0a5100;
	margin: 10px 0 0 0;
	padding: 12px 0 0 0;
	border-top: 1px solid #80b61b;
}

.footer .cont-block{
	color: #0a5100;
	margin: 10px 30px 0 30px;
	padding: 0;
	border-top: 1px solid #80b61b;
	position: relative;
	width: 690px;
}

.footer .menu-footer{
	margin: 20px 0 0 0;
}

.footer .menu-footer a:visited{
	color: #0a5100;
}

.footer .menu-footer a:hover, .footer .menu-footer a.active{
	color: #80b61b;
}

.footer .menu-footer a{
	margin: 0 0 0 0;
	padding: 0 40px 0 0;
	font-size: 14px;
	color: #0a5100;
	text-decoration: none;
}

.footer .twirl{
	position: absolute;
	top: -5px;
	left: 316px;
	z-index: 1;
}

.menu-top .twirl{
	position: absolute;
	top: 37px;
	left: 316px;
	z-index: 1;
}

.cont-text {
	line-height: 120%;
}

.cont-text p{
	padding: 0 0 0 0;
	margin: 0 30px 7px 0;
}

.cont-text .quote{
	padding: 15px 0 10px 0;
	margin: 20px 30px 20px 0;
	border-top: 1px solid #80b61b;
	border-bottom: 1px solid #80b61b;
	position: relative;
}

.cont-text .twirl{
	position: absolute;
	left: 316px;
	top: -5px;
}

.cont-text .quote p{
	padding: 0 0 0 0;
	margin: 0!important;
}

.cont-text ul{
	padding: 0 30px 0 10px;
	margin: 0;
	list-style: none;
}

.cont-text ul li{
	padding: 0 0 7px 20px;
	margin: 0;
	list-style-type: none;
	background:url('../images/bullet.png') left 4px no-repeat;
}

.cont-text ul li p{
	padding: 0 0 0 0;
	margin: 0!important;
}

.cont-text table{
	border-collapse: collapse;
